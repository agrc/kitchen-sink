{
  "name": "@ugrc/kitchen-sink",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/agrc/kitchen-sink.git"
  },
  "license": "MIT",
  "main": "",
  "workspaces": [
    "packages/dart-board",
    "packages/layer-selector",
    "packages/mouse-trap",
    "packages/sherlock",
    "packages/utilities"
  ],
  "scripts": {
    "build": "",
    "build-storybook": "build-storybook",
    "format": "prettier ./ --write",
    "graduate": "",
    "lint": "eslint ./ --ext .js --ignore-path ./.eslintignore",
    "lint:fix": "eslint ./ --ext .js --quiet --fix --ignore-path ./.eslintignore",
    "pre-release": "",
    "prepare": "",
    "release": "",
    "start": "",
    "storybook": "start-storybook -p 6006",
    "test": "npm run test --workspaces --if-present"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-packagejson"
    ],
    "semi": true,
    "singleQuote": true
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true,
      "node": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "plugins": [
      "react",
      "prettier",
      "import",
      "jsx-a11y",
      "react-hooks"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "plugin:react-hooks/recommended",
      "plugin:import/recommended",
      "plugin:jsx-a11y/recommended",
      "prettier"
    ]
  },
  "devDependencies": {
    "@arcgis/core": "^4.18.1",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "sass": "^1.32.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.2"
  }
}
