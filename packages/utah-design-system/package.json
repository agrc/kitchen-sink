{
  "name": "@ugrc/utah-design-system",
  "version": "1.5.2",
  "private": false,
  "description": "This is a collection of react components for implementing the [Utah Design System](https://designsystem.utah.gov).",
  "keywords": [
    "ugrc",
    "component",
    "esri",
    "arcgis",
    "react",
    "design-system",
