    "react-stately": "3.32.1",
    "tailwind-merge": "^2.4.0",
    "tailwind-variants": "^0.2.1"
  },
  "peerDependencies": {
    "@arcgis/core": "4.x",
    "firebase": "10.x",
    "prop-types": "^15.8.1",
    "react": ">=16.8",
    "react-dom": ">=16.8"
